{
  "name": "Page Benchmarker",
  "version": "1.3.2",
  "description": "Chromium Page Benchmarker.",
  "background_page": "background.html",
  "browser_action": {
    "default_title": "Benchmark page load time",
    "default_icon": "stopwatch.jpg"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["script.js"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
